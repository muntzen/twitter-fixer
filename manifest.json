{
    "name": "Twitter Fixer",
    "action": {},
    "manifest_version": 3,
    "version": "0.1",
    "description": "Removes the twitter status bar for me",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_icon": {
        "16": "images/icon16.png",
        "24": "images/icon24.png",
        "32": "images/icon32.png"
      }
    },
    "icons": { 
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png" 
    }
  }